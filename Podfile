# platform :ios, '9.0'

target 'WeatherCheck' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for WeatherCheck
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod 'GooglePlacePicker'
  pod 'Firebase/Core'
  
  
  
  target 'WeatherCheckTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'WeatherCheckUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
